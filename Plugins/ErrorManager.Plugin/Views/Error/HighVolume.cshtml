@model SharedPluginFeatures.BaseModel
@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer Localizer

@{
    ViewData["Title"] = "HighVolume";
}
<div class="bc">@Html.Raw(Model.BreadcrumbData())</div>

<div class="row">
    <div class="col-sm-12">

        <h2>@Localizer[nameof(Languages.LanguageStrings.UnableToProcessRequest)]</h2>
    </div>
</div>
<div class="row">
    <div class="col-sm-12">

        <div class="errorMessage">
            <p>@Localizer[nameof(Languages.LanguageStrings.PleaseTryAgainLater)]</p>


            <p>@Localizer[nameof(Languages.LanguageStrings.HighVolumeDescription)]</p>
        </div>
    </div>
</div>
