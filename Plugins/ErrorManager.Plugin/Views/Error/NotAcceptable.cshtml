@model SharedPluginFeatures.BaseModel
@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer Localizer

@{
    ViewData["Title"] = "NotAcceptable";
}

<div class="row">
    <div class="col-sm-12">

        <h2>@Localizer[nameof(Languages.LanguageStrings.NotAcceptable)]</h2>
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <div class="errorMessage">
            <p>@Localizer[nameof(Languages.LanguageStrings.NotAcceptableDescription)]</p>
        </div>
    </div>
</div>
